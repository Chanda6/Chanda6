<%@ Master Language="C#" AutoEventWireup="true"  CodeBehind="Admin.master.cs" Inherits="WebApplication17.Admin" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title></title>
    <style>
        .navbar
        {
            background: #00c3ff;
